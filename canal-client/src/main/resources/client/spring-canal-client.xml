<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="byName">

    <bean id="canalProcessor"
          class="com.jd.gms.canalclient.CanalClient" init-method="start" destroy-method="stop">
        <property name="destination" value="${canal.client.destination}"/>
        <property name="zkAddress" value="${canal.zookeeper.address}"/>
        <property name="processor" ref="processor"/>
    </bean>

    <bean id="processor" class="com.jd.gms.canalclient.processor.CanalProcessor"></bean>

</beans>
